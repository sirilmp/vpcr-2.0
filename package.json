{
  "name": "vpcr",
  "version": "1.0.7",
  "description": "A powerful Vite plugin that automatically tags React components with source reference attributes, enabling features like 'Alt + Click' to open source files in your IDE.",
  "homepage": "https://vpcr.vercel.app",
  "changelog": "https://vpcr.vercel.app/changelog",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup src/index.ts --format cjs,esm --dts --clean",
    "dev": "tsup src/index.ts --format cjs,esm --watch --dts",
    "lint": "tsc",
    "test": "vitest run"
  },
  "keywords": [
    "vite",
    "vite-plugin",
    "react",
    "babel",
    "tagger",
    "source-reference",
    "click-to-open",
    "antigravity",
    "cursor"
  ],
  "author": {
    "name": "Siril M Philip",
    "email": "sirilmp.online@gmail.com",
    "url": "sirilmp.com"
  },
  "license": "MIT",
  "peerDependencies": {
    "vite": "^5.0.0 || ^6.0.0 || ^7.0.0"
  },
  "dependencies": {
    "@babel/generator": "^7.23.0",
    "@babel/parser": "^7.23.0",
    "@babel/traverse": "^7.23.0",
    "@babel/types": "^7.23.0",
    "launch-editor": "^2.12.0",
    "lucide-react": "^0.564.0"
  },
  "devDependencies": {
    "@types/babel__generator": "^7.6.0",
    "@types/babel__traverse": "^7.20.0",
    "@types/node": "^20.10.0",
    "tsup": "^8.0.0",
    "typescript": "^5.3.0",
    "vite": "^5.0.0",
    "vitest": "^3.0.0"
  }
}
